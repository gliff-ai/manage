{
  "name": "@gliff-ai/manage",
  "version": "0.3.11",
  "description": "gliff.ai MANAGE",
  "repository": "git://github.com/gliff-ai/manage.git",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx vite build -c vite.config.lib.ts && tsc --emitDeclarationOnly --outDir dist && tsc-alias -p tsconfig.json",
    "watch": "npx vite build -c vite.config.lib.ts --watch",
    "format": "npx --no-install prettier --write src",
    "prettier": "npx --no-install prettier -c src",
    "lint": "tsc --noEmit && npm run lint:ts && npm run prettier",
    "lint:ts": "npx --no-install eslint src",
    "serve": "vite",
    "build:example": "vite build",
    "test": "jest --coverage --passWithNoTests"
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@gliff-ai/style": "^1.0.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "history": "^5.0.1",
    "jest": "^27.0.4",
    "prettier": "==2.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.4",
    "tsc-alias": "^1.3.9",
    "typescript": "^4.2.5",
    "vite": "^2.5.3",
    "vite-aliases": "^0.8.6"
  },
  "peerDependencies": {
    "@gliff-ai/style": "^1.0.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "history": "^5.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.3"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "react-inlinesvg": "^2.3.0"
  }
}
